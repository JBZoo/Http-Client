{
    "name"              : "jbzoo/http-client",
    "type"              : "library",
    "description"       : "Simple HTTP-client, wrapper for Guzzle and rmccue/requests",
    "license"           : "MIT",
    "keywords"          : [
        "http", "https", "client", "guzzle", "guzzlehttp", "curl", "request", "requests", "rmccue"
    ],
    "minimum-stability" : "stable",
    "authors"           : [
        {
            "name"  : "SmetDenis",
            "email" : "denis@jbzoo.com",
            "role"  : "lead"
        }
    ],
    "require"           : {
        "php"             : ">=7.0",
        "jbzoo/data"      : "^1.4",
        "rmccue/requests" : "^1.6"
    },
    "suggest"           : {
        "guzzlehttp/guzzle" : "For PHP 5.4+ please use versions ^5.3|^6.3"
    },
    "require-dev"       : {
        "jbzoo/phpunit" : "^2.0",
        "jbzoo/utils"   : "^1.7.8|^2.0"
    },
    "autoload"          : {
        "psr-4" : {
            "JBZoo\\HttpClient\\" : "src"
        }
    },
    "extra"             : {
        "branch-alias" : {
            "dev-master" : "1.x-dev"
        }
    },
    "config"            : {
        "optimize-autoloader" : true
    },
    "support"           : {
        "issues" : "https://github.com/JBZoo/Http-Client/issues"
    }
}
